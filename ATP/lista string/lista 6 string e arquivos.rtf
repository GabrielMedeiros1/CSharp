\par
        Console.Write("Informe o valor do aluguel por ve\'edculo: ");\par
        double valorAluguel = double.Parse(Console.ReadLine());\par
\par
        //chama as fun\'e7\'f5es para fazer os c\'e1lculos\par
        double faturamentoAnual = CalcularFaturamentoAnual(quantidadeVeiculos, valorAluguel);\par
        double valorMultasMensal = CalcularValorMultasMensal(quantidadeVeiculos, valorAluguel);\par
        double valorManutencaoAnual = CalcularValorManutencaoAnual(quantidadeVeiculos); \par
\par
        Console.WriteLine("Faturamento Anual: R$" + faturamentoAnual);\par
        Console.WriteLine("Valor das Multas no M\'eas: R$" + valorMultasMensal);\par
        Console.WriteLine("Valor da Manuten\'e7\'e3o Anual: R$" + valorManutencaoAnual);\par
\par
        GravarResultadoEmArquivo(faturamentoAnual, valorMultasMensal, valorManutencaoAnual);\par
\par
        Console.WriteLine("Resultados gravados no arquivo resultado.txt.");\par
    \}\par
\par
    static double CalcularFaturamentoAnual(int quantidadeVeiculos, double valorAluguel) //calcula o faturamento anual\par
    \{\par
        int veiculosAlugadosPorMes = quantidadeVeiculos / 3;\par
        int mesesNoAno = 12;\par
        return veiculosAlugadosPorMes * valorAluguel * mesesNoAno;\par
    \}\par
\par
    static double CalcularValorMultasMensal(int quantidadeVeiculos, double valorAluguel) //calcula o valor mensal das multas\par
    \{\par
        int veiculosDevolvidosAtrasoPorMes = quantidadeVeiculos / 10;\par
        double percentualMulta = 0.2;\par
        return veiculosDevolvidosAtrasoPorMes * valorAluguel * percentualMulta;\par
    \}\par
\par
    static double CalcularValorManutencaoAnual(int quantidadeVeiculos) //calcula o valor da manunten\'e7\'e3o anual\par
    \{\par
        double percentualManutencao = 0.02;\par
        double valorManutencaoMedia = 600.00;\par
        return quantidadeVeiculos * percentualManutencao * valorManutencaoMedia;\par
    \}\par
\par
    static void GravarResultadoEmArquivo(double faturamentoAnual, double valorMultasMensal, double valorManutencaoAnual)\par
    \{\par
        using (StreamWriter writer = new StreamWriter("resultado.txt")) //sa\'edda do resultado\par
        \{\par
            writer.WriteLine(faturamentoAnual);\par
            writer.WriteLine(valorMultasMensal);\par
            writer.WriteLine(valorManutencaoAnual);\par
        \}\par
    \}\par
