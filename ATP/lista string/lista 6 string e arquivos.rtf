    static string ObterLetras(int quantidade)\par
    \{\par
        string letras = "";\par
        for (int i = 1; i <= quantidade; i++)\par
        \{\par
            Console.Write("Informe a letra #" + i + ": ");\par
            char letra = Console.ReadLine()[0];\par
            letras += letra;\par
        \}\par
        return letras;\par
    \}\par
\par
