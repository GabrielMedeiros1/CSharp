using System;\par
using System.IO;\par
\par
class Program\par
\{\par
    static void Main(string[] args)\par
\{\par
    Console.WriteLine("Digite um n\'famero inteiro:");\par
    int numero = int.Parse(Console.ReadLine());\par
\par
    int somaDivisores = CalcularDivisores(numero);\par
\par
    Console.WriteLine("Divisores:");\par
    ImprimirDivisores(numero);\par
\par
    SalvarSomaDivisores(numero, somaDivisores);\par
\par
    Console.WriteLine("Soma dos divisores salva no arquivo.");\par
\}\par
\par
static int CalcularDivisores(int numero)\par
\{\par
    int soma = 0;\par
\par
    // Percorre todos os n\'fameros de 1 at\'e9 o n\'famero dado\par
    for (int i = 1; i <= numero; i++)\par
    \{\par
        // Verifica se o n\'famero \'e9 divis\'edvel por i\par
        if (numero % i == 0)\par
        \{\par
            // Se for divis\'edvel, adiciona i \'e0 soma dos divisores\par
            soma += i;\par
        \}\par
    \}\par
\par
    // Retorna a soma dos divisores\par
    return soma;\par
\}\par
\par
static void ImprimirDivisores(int numero)\par
\{\par
    // Percorre todos os n\'fameros de 1 at\'e9 o n\'famero dado\par
    for (int i = 1; i <= numero; i++)\par
    \{\par
        // Verifica se o n\'famero \'e9 divis\'edvel por i\par
        if (numero % i == 0)\par
        \{\par
            // Se for divis\'edvel, imprime i na tela\par
            Console.WriteLine(i);\par
        \}\par
    \}\par
\}\par
\par
static void SalvarSomaDivisores(int numero, int somaDivisores)\par
\{\par
    string nomeArquivo = "soma_divisores.txt";\par
\par
    // Cria um objeto StreamWriter para escrever no arquivo\par
    using (StreamWriter writer = new StreamWriter(nomeArquivo))\par
    \{\par
        // Escreve as informa\'e7\'f5es no arquivo\par
        writer.WriteLine("N\'famero: \{0\}",numero);\par
        writer.WriteLine("Soma dos divisores: \{0\}",somaDivisores);\par
    \}\par
\}\par
\}\par
