class Program\par
\{\par
    static void Main(string[] args)\par
    \{\par
        Console.WriteLine("Digite uma frase:");\par
        string frase = Console.ReadLine();\par
\par
        string fraseCodificada = CodificarCesar(frase, 3);\par
\par
        Console.WriteLine($"Frase codificada: \{fraseCodificada\}");\par
    \}\par
\par
    static string CodificarCesar(string frase, int deslocamento)\par
    \{\par
        string novaFrase = "";\par
\par
        foreach (char caractere in frase)\par
        \{\par
            if (char.IsLetter(caractere))\par
            \{\par
                char caractereCodificado = (char)(caractere + deslocamento);\par
\par
                if (!char.IsLetter(caractereCodificado))\par
                \{\par
                    caractereCodificado = (char)(caractereCodificado - 26);\par
                \}\par
\par
                novaFrase += caractereCodificado;\par
            \}\par
            else\par
            \{\par
                novaFrase += caractere;\par
            \}\par
        \}\par
\par
        return novaFrase;\par
    \}\par
\}\par
