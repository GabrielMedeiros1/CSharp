using System;\par
using System.IO;\par
\par
class Program\par
\{\par
    static void Main()\par
    \{\par
        string nomeArquivo = "arquivo.txt"; // Nome do arquivo a ser lido\par
\par
        // Verifica se o arquivo existe\par
        if (!File.Exists(nomeArquivo))\par
        \{\par
            Console.WriteLine("O arquivo n\'e3o existe.");\par
            return;\par
        \}\par
\par
        string[] linhas = File.ReadAllLines(nomeArquivo); // L\'ea todas as linhas do arquivo\par
        int quantidadeValores = linhas.Length;\par
\par
        // Verifica se o arquivo est\'e1 vazio\par
        if (quantidadeValores == 0)\par
        \{\par
            Console.WriteLine("O arquivo est\'e1 vazio.");\par
            return;\par
        \}\par
\par
        double valorMaximo = double.MinValue;\par
        double valorMinimo = double.MaxValue;\par
        double somaValores = 0;\par
\par
        // Percorre todas as linhas do arquivo\par
        foreach (string linha in linhas)\par
        \{\par
            if (double.TryParse(linha, out double valor))\par
            \{\par
                // Atualiza o valor m\'e1ximo\par
                if (valor > valorMaximo)\par
                    valorMaximo = valor;\par
\par
                // Atualiza o valor m\'ednimo\par
                if (valor < valorMinimo)\par
                    valorMinimo = valor;\par
\par
                // Soma os valores\par
                somaValores += valor;\par
            \}\par
        \}\par
\par
        double media = somaValores / quantidadeValores;\par
\par
        // Imprime os resultados\par
        Console.WriteLine("Valor m\'e1ximo: " + valorMaximo);\par
        Console.WriteLine("Valor m\'ednimo: " + valorMinimo);\par
        Console.WriteLine("M\'e9dia: " + media);\par
    \}\par
\}\par
\par
}
