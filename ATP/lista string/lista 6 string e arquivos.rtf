    \{\par
        using (StreamWriter writer = new StreamWriter("letras.txt"))\par
        \{\par
            writer.WriteLine(letras);\par
        \}\par
    \}\par
\par
    static int ContarVogais(string letras)\par
    \{\par
        int quantidadeVogais = 0;\par
        foreach (char letra in letras.ToLower())\par
        \{\par
            if (letra == 'a' || letra == 'e' || letra == 'i' || letra == 'o' || letra == 'u')\par
            \{\par
                quantidadeVogais++;\par
            \}\par
        \}\par
        return quantidadeVogais;\par
    \}\par
